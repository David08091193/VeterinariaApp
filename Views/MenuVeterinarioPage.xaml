<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="VeterinariaApp.Views.MenuVeterinarioPage"
             Title="Menú Veterinario">
    <StackLayout Padding="20" Spacing="15">
        <Label Text="Bienvenido Veterinario" FontSize="24" HorizontalOptions="Center" />
        <Button Text="Historial Médico" Clicked="OnHistorialMedicoClicked" />
        <Button Text="Agenda de Citas" Clicked="OnAgendaCitasClicked" />
        <Button Text="Cerrar sesión" BackgroundColor="Red" TextColor="White" Clicked="OnCerrarSesionClicked" />

    </StackLayout>
</ContentPage>
